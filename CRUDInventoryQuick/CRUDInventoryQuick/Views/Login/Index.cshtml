@model IEnumerable<CRUDInventoryQuick.Models.ASPNETUSERLOGIN>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.LoginProvider)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ProviderKey)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ASPNETUSER_ASPNETUSER)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.LoginProvider)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ProviderKey)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ASPNETUSER_ASPNETUSER.ASPNETUSER_ID)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
